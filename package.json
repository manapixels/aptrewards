{
  "name": "aptrewards",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "keywords": [
    "nextjs",
    "typescript"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "aptos:compile": "aptos move compile --package-dir move/ --named-addresses aptrewards_addr=fdf10dc600959de16b94a042e523c6d91ce0a0c5039ddb06fa71c72e7bf0ba2e",
    "aptos:deploy": "aptos move deploy-object --address-name aptrewards_addr --package-dir move/ --profile default",
    "aptos:deploy_and_init": "tsx move/deploy_and_initialize.ts",
    "aptos:create_test_program": "tsx move/create_test_program.ts",
    "aptos:upgrade": "tsx scripts/upgrade.ts",
    "aptos:test": "aptos move test --package-dir move/ --named-addresses aptrewards_addr=123"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.28.0",
    "@aptos-labs/wallet-adapter-react": "^3.6.1",
    "@hookform/resolvers": "^3.9.0",
    "@martianwallet/aptos-wallet-adapter": "^0.0.5",
    "@okwallet/aptos-wallet-adapter": "^0.0.7",
    "@pontem/wallet-adapter-plugin": "^0.2.1",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@tanstack/react-table": "^8.20.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "fewcha-plugin-wallet-adapter": "^0.1.3",
    "html5-qrcode": "^2.3.8",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.436.0",
    "next": "14.2.6",
    "next-themes": "^0.3.0",
    "qrcode.react": "^4.0.1",
    "react": "^18",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "aptos": "^1.21.0",
    "dotenv": "^16.4.5",
    "eslint": "^8",
    "eslint-config-next": "14.2.6",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tree-kill": "^1.2.2",
    "tsx": "^4.19.0",
    "typescript": "^5"
  },
  "browser": {
    "child_process": false
  }
}
